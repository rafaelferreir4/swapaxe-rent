<div class="border-b border-gray-200">
  {% if primary %}
    <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
    <nav class="-mb-px flex space-x-8" aria-label="Tabs">{{ primary }}</nav>
  {% endif %}
  {% if secondary %}
    <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
    <nav class="-mb-px flex space-x-8" aria-label="Tabs">{{ primary }}</nav>
  {% endif %}
</div>
